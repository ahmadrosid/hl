let read_string = |l: &mut Lexer, ch: char| -> Vec<char> {
    let position = l.position;
    l.read_char();
    while l.position < l.input.len() && l.ch != ch {
        if l.ch == '\\' { l.read_char() }
        l.read_char();
    }
    l.read_char();
    if l.position > l.input.len() {
        l.position = l.position - 1;
        l.read_position -= 1;
    }
    l.input[position..l.position].to_vec()
};

